<linked-list>
    <AlertDefine>
        <id>40001</id>
        <title>UPS关机</title>
        <category>4</category>
        <level>0</level>
        <descriptionPattern>{l:name}报警</descriptionPattern>
        <rule class="Equal">
            <name>UPS关机</name>
            <limit>0</limit>
            <sensorValue class="SimpleSensorValue">
                <deviceId>d1a53f97-74e4-4e56-b8c7-0e3d57e54a8c</deviceId>
                <propertyPath>MR33.Run.State</propertyPath>
            </sensorValue>
        </rule>
        <enable>true</enable>
    </AlertDefine>
    <AlertDefine>
        <id>40002</id>
        <title>UPS切换为电池输出</title>
        <category>4</category>
        <level>0</level>
        <descriptionPattern>{l:name}报警</descriptionPattern>
        <rule class="Equal">
            <name>UPS切换为电池输出</name>
            <limit>5</limit>
            <sensorValue class="SimpleSensorValue">
                <deviceId>d1a53f97-74e4-4e56-b8c7-0e3d57e54a8c</deviceId>
                <propertyPath>MR33.Run.State</propertyPath>
            </sensorValue>
        </rule>
        <enable>true</enable>
    </AlertDefine>
    <AlertDefine>
        <id>40003</id>
        <title>UPS485异常</title>
        <category>4</category>
        <level>0</level>
        <descriptionPattern>{l:name}报警</descriptionPattern>
        <rule class="Equal">
            <name>UPS485异常</name>
            <limit>1</limit>
            <sensorValue class="SimpleSensorValue">
                <deviceId>d1a53f97-74e4-4e56-b8c7-0e3d57e54a8c</deviceId>
                <propertyPath>MR33.RS485.Alarm</propertyPath>
            </sensorValue>
        </rule>
        <enable>true</enable>
    </AlertDefine>
</linked-list>

