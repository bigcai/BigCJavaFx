<linked-list>
  <AlertDefine>
    <id>21005</id>
    <title>烟感报警</title>
    <category>2</category>
    <level>0</level>
    <descriptionPattern>{l:name}报警</descriptionPattern>
    <rule class="Equal">
      <name>环境主机烟感</name>
      <limit>1</limit>
      <sensorValue class="SimpleSensorValue">
        <deviceId>e1a84bf5-f0ab-478c-a3dd-b0c7df039fce</deviceId>
        <propertyPath>InOne.Smoke.state</propertyPath>
      </sensorValue>
    </rule>
    <enable>true</enable>
  </AlertDefine>
